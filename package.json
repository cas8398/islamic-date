{
  "name": "islamic-date",
  "version": "0.1.8",
  "description": "Hijri calendar with Umm Al-Qura & MABIMS conversion, Islamic events, Javanese calendar. Multi-language (Arabic, Indonesian, Javanese, English).",
  "main": "dist/islamic-date.cjs",
  "module": "dist/islamic-date.esm.js",
  "browser": "dist/islamic-date.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "prepublishOnly": "npm run test && npm run build",
    "publish:dry": "npm publish --dry-run",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "hijri",
    "islamic",
    "date",
    "calendar",
    "umm-al-qura",
    "muslim",
    "arabic",
    "indonesian",
    "ramadan",
    "eid"
  ],
  "author": "Cahyanudien",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cas8398/islamic-date.git"
  },
  "bugs": {
    "url": "https://github.com/cas8398/islamic-date/issues"
  },
  "homepage": "https://github.com/cas8398/islamic-date#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^29.7.0",
    "rollup": "^4.6.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/islamic-date.esm.js",
      "require": "./dist/islamic-date.cjs",
      "default": "./dist/islamic-date.esm.js"
    }
  }
}
